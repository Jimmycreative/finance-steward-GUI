{
  "name": "finance-steward",
  "version": "0.0.1",
  "description": "Powered by typescript, react, redux, webpack, electron, mongoose, express, swagger",
  "author": "mark",
  "mail": "shawninjuly@gmail.com",
  "license": "MIT",
  "scripts": {
    "clear": "ts-node scripts/clear_non-ts-files.ts",
    "electorn:main": "cross-env NODE_ENV=development   electron -r ./babel_register ./src/main/index.ts",
    "electron:start": "cross-env NODE_ENV=development webpack serve --config webpack.config.renderer.babel.js --inline",
    "electron:restart": "rm -rf ./src/data/* | yarn electron:start",
    "express:build": "yarn clear && tsoa spec-and-routes && tsc",
    "express:start": "node build/src/api/server.js",
    "express:dev": "concurrently 'nodemon' 'nodemon -x tsoa spec-and-routes'",
    "koa2:dev": "tsc --watch & export NODE_ENV=development && node src/koa2/app.js -t dist/ -e dist/app.js",
    "koa2:build": "rm -rf dist/* && tsc"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.26",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/express": "^4.17.11",
    "body-parser": "^1.19.0",
    "electron": "^12.0.5",
    "express": "^4.17.1",
    "image-size": "^1.0.0",
    "koa2": "^2.0.0-alpha.7",
    "mongoose": "^5.12.4",
    "mongoose-autopopulate": "^0.13.0",
    "mongoose-to-swagger": "^1.3.0",
    "multer": "^1.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.16",
    "@types/electron": "^1.6.10",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-thunk": "^2.1.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/webpack-env": "^1.16.0",
    "chai": "^4.3.4",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.2",
    "electron-reloader": "^1.2.0",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.7",
    "redux-logger": "^3.0.6",
    "simple-node-logger": "^18.12.24",
    "source-map-loader": "^2.0.1",
